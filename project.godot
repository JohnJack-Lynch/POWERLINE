config_version=5

[application]

config/name="POWERLINE"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.0627451, 0.101961, 0.278431, 1)
boot_splash/image="res://pl_splash.png"
config/icon="res://pl_icon.svg"
config/show_image=true

[audio]

buses/default_bus_layout="res://Sounds/default_bus_layout.tres"

[autoload]

SignalBus="*res://Scripts/Global/signal_bus.gd"
SceneChanger="*res://Scripts/Global/scene_changer.gd"

[display]

